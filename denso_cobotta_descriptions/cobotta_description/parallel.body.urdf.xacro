<?xml version="1.0" encoding="UTF-8"?>
<robot name="cobotta" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:macro name="cobotta_gripper"
               params="namespace">
	<link name="${namespace}gripper_base">
		<visual>
			<origin rpy="0 0 0" xyz="0.000000 0.000000 0.000000"/>
			<geometry>
				<mesh filename="package://denso_cobotta_descriptions/cobotta_description/gripper_base.dae" scale="1 1 1"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_cobotta_descriptions/cobotta_description/gripper_base.dae" scale="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1"/>
			<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.000000"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
	</link>
	<joint name="${namespace}joint_gripper_base" type="fixed">
		<parent link="${namespace}J6"/>
		<child link="${namespace}gripper_base"/>
		<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.0000000"/>
	</joint>
	<link name="${namespace}left_finger">
		<visual>
			<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 -0.060000"/>
			<geometry>
				<mesh filename="package://denso_cobotta_descriptions/cobotta_description/left_finger.dae" scale="1 1 1"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0.000000 0.000000 -0.060000"/>
			<geometry>
				<mesh filename="package://denso_cobotta_descriptions/cobotta_description/left_finger.dae" scale="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1"/>
			<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.000000"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
	</link>
	<joint name="${namespace}joint_gripper" type="prismatic">
		<parent link="${namespace}gripper_base"/>
		<child link="${namespace}left_finger"/>
		<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.06000"/>
		<axis xyz="-0.000000 1.000000 -0.000000"/>
		<limit effort="20" lower="0.000000" upper="0.015000" velocity="0.040000"/>
		<dynamics damping="0" friction="0"/>
	</joint>
	<transmission name="${namespace}trans_gripper">
		<type>transmission_interface/SimpleTransmission</type>
		<joint name="${namespace}joint_gripper">
			<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
		</joint>
		<actuator name="${namespace}motor_gripper">
			<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>
	<link name="${namespace}right_finger">
		<visual>
			<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 -0.060000"/>
			<geometry>
				<mesh filename="package://denso_cobotta_descriptions/cobotta_description/right_finger.dae" scale="1 1 1"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0.000000 0.000000 -0.060000"/>
			<geometry>
				<mesh filename="package://denso_cobotta_descriptions/cobotta_description/right_finger.dae" scale="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1"/>
			<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.000000"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
	</link>
	<joint name="${namespace}joint_gripper_mimic" type="prismatic">
		<parent link="${namespace}gripper_base"/>
		<child link="${namespace}right_finger"/>
		<mimic joint="${namespace}joint_gripper" multiplier="-1.000000" offset="0.000000"/>
		<origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.060000"/>
		<axis xyz="-0.000000 1.000000 -0.000000"/>
		<limit effort="20" lower="-0.015000" upper="0.000000" velocity="0.040000"/>
		<dynamics damping="0" friction="0"/>
	</joint>
  </xacro:macro>
</robot>
